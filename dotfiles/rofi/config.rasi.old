//@theme "catppuccin-mocha"
@theme "/dev/null"





* {
  border: 0;
  margin: 0;
  padding: 0;
  spacing: 0;
  width: 1000;

  bg: #363a4f; //last 2 digits are alpha value 7f is 127 -- 0x32 for 50
  bg-alt: #464b64;
  fg: #cad3f5;
  fg-alt: #8aadf4;
  accent: #8aadf4;
  accent-alt: #ee99a0;
  transparent: #00000000;
  //transparent: #00000032;

  background-color: @bg;
  //background-color: @transparent;
  text-color: @fg;
}



element-text, element-icon , mode-switcher {
    background-color: inherit;
    text-color:       inherit;
}


window {
    //width: 45%;
    background-color: @bg;
    border:           1;
    fullscreen: false;
    padding:          5;
    transparency: "real";
    //transparency: "screenshot";
}
mainbox {
    border:  3;
    background-color: @bg;
    padding: 0;
    children: [inputbar, listview];
}
message {

    background-color: @bg-alt;
    margin: 2px;
    border:       1px dash 0px 0px ;
    //border-color: @separatorcolor;
    padding:      1px ;
}
textbox {
    text-color: @fg;
}
scrollbar {
    width:        4px ;
    border:       0;
    handle-width: 8px ;
    padding:      5;
    //handle-color: @bg-alt;
    handle-color: @bg; //
}
listview {
    columns: 2;
    background-color: @bg;
    scrollbar: false;
    scrollbar-width: 0;
    cycle: true;
    fixed-height: false;
    dynamic: false; // DYNAMIC
    border:       2px dash 0px 0px ;
    //border-color: @separatorcolor;
    spacing:      5px ;
    scrollbar:    true;
    padding:      2px 0px 0px ;
    lines: 6;
    require-input: false;
}
element {
    border:  0;
    padding: 3px ;
    children: [element-icon, element-text];
}


element selected {
    background-color: @bg-alt;
    text-color: @fg-alt;
}


element-text {
    background-color: inherit;
    text-color:       inherit;
    padding: 10px 10px;
    vertical-align: 0.5;
    //highlight: underline red;
}
element-text selected {
  //text-color: @fg-alt;
  text-color: @accent-alt;
}

element-icon {
  padding: 10px 10px;
  size: 1.25em;
}

element.normal.normal {
    background-color: @bg;
    //background-color: @transparent;
    text-color:       @fg;
}
element.normal.active {
    background-color: @bg-alt;
    //background-color: @transparent;
    text-color:       @fg-alt;
}
element.selected.normal {
    //background-color: @bg-alt;
    //background-color: @bg; //
    background-color: @bg-alt; //
    //background-color: @transparent;
    text-color:       @fg-alt;
}
element.selected.active {
    background-color: @bg-alt;
    //background-color: @transparent;
    text-color:       @fg-alt;
}
mode-switcher {
    spacing: 0;
    border:       2px dash 0px 0px ;
    //border-color: @separatorcolor;
}


button {
    padding: 10px;
    background-color: @bg-alt;
    text-color: @bg;
    vertical-align: 0.5; 
    horizontal-align: 0.5;
}



button.selected {
    background-color: @bg-alt;
    text-color:       @fg-alt;
}
inputbar {
    spacing:    3;
    //background-color: @bg;
    background-color: @bg-alt; //
    text-color: @fg;
    padding:    3px ;
    //children: [prompt, entry, case-indicator];
    children: [prompt, entry];
}
case-indicator {
    spacing:    0;
    text-color: @fg;
}
entry {
    spacing:    0;
    text-color: @fg;
    background-color: inherit;
    padding: 12px 5px;
    //margin: 20px 0px 0px 10px;
    margin: 1px 0px 0px 10px;
    cursor-color: @fg;
    cursor-width: 8px;
    blink: false;
}
prompt {
    spacing:    0;
    //text-color: @fg;
    text-color: @bg;
    //background-color: inherit;
    background-color: @accent;
    padding: 12px;
}
textbox-prompt-colon {
    expand:     false;
    str:        ":";
    margin:     0px 0.3em 0em 0em ;
    text-color: @fg;
}




